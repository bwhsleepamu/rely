<% @title = "Result: #{@result.name}" %>
<div class="page-header">
  <h1>
    <%= @title %>
    <%= link_to "Edit Result", edit_result_path(@result), class: 'btn btn-xs' %>
    <%= link_to "Delete Result", @result, method: :delete, class: 'btn btn-xs btn-danger', data: { confirm: "Are you sure you want to delete Result #{@result.name}?" } %>
    <%= link_to "View Results", results_path, class: 'btn btn-xs' %>
  </h1>
</div>

<dl class="dl-horizontal">
  <dt>User</dt>
  <dd><%= link_to @result.user.name, @result.user if @result.user %></dd>

  <dt>Study</dt>
  <dd><%= link_to @result.study.name, @result.study if @result.study %></dd>

  <dt>Exercise</dt>
  <dd><%= link_to @result.exercise.name, @result.exercise if @result.exercise %></dd>

  <dt>Rule</dt>
  <dd><%= link_to @result.rule.name, @result.rule if @result.rule %></dd>

  <dt>Result Type</dt>
  <dd><%= @result.result_type %></dd>

  <dt>Location</dt>
  <dd><%= @result.location %></dd>

  <dt>Deleted</dt>
  <dd><%= @result.deleted %></dd>

</dl>
