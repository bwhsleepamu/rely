<h1>Listing studies</h1>

<table>
  <tr>
    <th>Original</th>
    <th>Type</th>
    <th>Location</th>
    <th>Deleted</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @studies.each do |study| %>
  <tr>
    <td><%= study.original_id %></td>
    <td><%= study.type_id %></td>
    <td><%= study.location %></td>
    <td><%= study.deleted %></td>
    <td><%= link_to 'Show', study %></td>
    <td><%= link_to 'Edit', edit_study_path(study) %></td>
    <td><%= link_to 'Destroy', study, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Study', new_study_path %>
